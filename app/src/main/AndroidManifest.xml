<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.payrollproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddGajiOnClicked"></activity>
        <activity android:name=".AddGaji" />
        <activity android:name=".EditGaji" />
        <activity android:name=".GajiKaryawan" />
        <activity android:name=".AddAbsensiFormAfterClicked" />
        <activity android:name=".EditAbsensi" />
        <activity android:name=".AddAbsensi" />
        <activity android:name=".DeleteKaryawan" />
        <activity android:name=".AbsensiKaryawan" />
        <activity android:name=".AddKaryawan" />
        <activity android:name=".InformasiKaryawan" />
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="http"
                    android:host="www.giorgos.nl"
                    android:pathPrefix="/cv"/>
                <data android:scheme="cv"
                    android:host="giorgos"/>
            </intent-filter>
        </activity>

    </application>

</manifest>